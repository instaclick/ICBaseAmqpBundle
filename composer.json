{
    "name": "instaclick/base-amqp-bundle",
    "description": "This bundle provides basic amqp support for Dating Engine",
    "keywords": ["base", "amqp", "RabbitMQ"],
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "InstaClick Inc.",
            "homepage": "http://www.instaclick.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "http://satis.dev:8080/"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "ext-amqp": "~1.2",
        "symfony/symfony": "2.2.*"
    },
    "require-dev": {
        "phpunit/PHPUnit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "IC\\Bundle\\Base\\AmqpBundle": "" }
    },
    "target-dir": "IC/Bundle/Base/AmqpBundle",
    "minimum-stability": "dev"
}