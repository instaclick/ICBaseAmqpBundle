{
    "name": "instaclick/base-amqp-bundle",
    "description": "This bundle provides basic amqp support to Symfony 2 applications",
    "keywords": ["base", "amqp", "RabbitMQ"],
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "InstaClick Inc.",
            "homepage": "http://www.instaclick.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "ext-amqp": ">=1.2",
        "symfony/symfony": ">=2.2"
    },
    "require-dev": {
        "phpunit/PHPUnit": "3.7.*",
        "instaclick/base-test-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": { "IC\\Bundle\\Base\\AmqpBundle": "" }
    },
    "target-dir": "IC/Bundle/Base/AmqpBundle",
    "minimum-stability": "dev"
}
